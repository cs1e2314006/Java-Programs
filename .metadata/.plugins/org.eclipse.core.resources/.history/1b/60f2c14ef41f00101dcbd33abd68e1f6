package AbstractClasses;

public class Fourth extends Third{
	
		void method1() {
			System.out.println("Method1 from FirstClass");
		}
		void method2() {
			System.out.println("Method2 from FirstClass");
		}void method3() {
			System.out.println("Method3 from FirstClass");
		}void method4() {
			System.out.println("Method4 from FirstClass");
		}void method5() {
			System.out.println("Method5 from FirstClass");
		}

	public static void main(String[] args) {
		Fourth a1=new Fourth();
		a1.method1();
		SecondClass a2=new Fourth();
		a2.method2();
	}
}
