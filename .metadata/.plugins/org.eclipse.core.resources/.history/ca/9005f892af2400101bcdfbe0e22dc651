package Arraydemo;

public class Array2 {
	int arr1[][]=new int [3][2];
	int arr2[][]= {{1,2,3},{4,5,6}};
	public Array2() {
		// TODO Auto-generated constructor stub
		for(int i=0;i<3;i++)
		{
			for(int j=0;j<2;j++)
				this.arr1[i][j]=(i+1)*j;
		}
	}
	
	void print(int arr[][],int row,int column) {
		for(int i=0;i<row;i++)
		{
			for(int j=0;j<column;j++)
				this.arr1[i][j]=(i+1)*j;
		}
	}
	
}
