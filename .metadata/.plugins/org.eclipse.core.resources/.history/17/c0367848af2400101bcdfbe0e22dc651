package Arraydemo;

public class Starter {
	public static void main(String[] args) {
		int[] arr= {10,4,6,3,11};
		for (int i=0;i<arr.length;i++)
			arr[i]=i+1;
		Array1 array1=new Array1();
		array1.display(arr);
		
		int x=array1.multiply(arr);
		System.out.println("Multiplication of all the numbers is "+x);
		
//		System.out.println();
		array1.Quicksort(arr, 0, 4);
		array1.display(arr);
}
}
