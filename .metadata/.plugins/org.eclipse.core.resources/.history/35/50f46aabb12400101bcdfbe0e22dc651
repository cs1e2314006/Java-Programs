	        arr3[0] = new int[2];
	        arr3[1] = new int[3];
	        arr3[2] = new int[4];
	        arr3[3] = new int[5];
	        arr3[4] = new int[2];
	        int value = 1;
	        for (int i = 0; i < arr3.length; i++) {
	            for (int j = 0; j < arr3[i].length; j++) {
	                arr3[i][j] = value++;
	            }
	        }

	        // Optional: Print the array structure
	        for (int i = 0; i < arr3.length; i++) {
	            for (int j = 0; j < arr3[i].length; j++) {
	                arr3[i][j] = i + j; // Assign some values
	                System.out.print(arr3[i][j] + " ");
	            }
	            System.out.println();
	        }
